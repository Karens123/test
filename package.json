{
  "name": "wenwen-app-admin",
  "version": "0.0.1",
  "description": "wenwen app admin. Base on React Ant.Design",
  "main": "index.js",
  "scripts": {
    "precommit-msg-1": "echo 'Pre-commit checks...'",
    "eslint": "eslint --cache --ext .js src",
    "test": "cross-env NODE_PATH=./src mocha tests/.setup.js tests/**/*.test.js --compilers css:mocha-compiler.js",
    "packvendor": "cross-env NODE_ENV=production webpack --config build/webpack-vendor-dll.js && webpack --config build/webpack-ui-dll.js",
    "build:prod": "webpack --config build/webpack-prod.js",
    "build:test": "webpack --config build/webpack-test.js",
    "start": "node build/server",
    "debug": "cross-env DEBUG=express:* node build/server",
    "fstart:prod": "npm run build:prod && cross-env NODE_ENV=production WENWEN_ENV=prod  forever start build/server.js",
    "fstart:test": "npm run build:test && cross-env NODE_ENV=production WENWEN_ENV=test forever start build/server.js",
    "fstop": "forever stop build/server.js"
  },
  "pre-commit": [
    "precommit-msg-1"
  ],
  "author": "linzhaoming",
  "license": "WTFPL",
